<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		id="selectDialog"
		title="Country"
		items="{/Country}"
		search=".onValueHelpSearch"
		confirm=".onValueHelpClose"
		cancel=".onValueHelpClose">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{Country}"
            description="{Country_Text}"
			 />
	</SelectDialog>
</core:FragmentDefinition>